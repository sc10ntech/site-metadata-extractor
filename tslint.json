{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "no-namespace": [true, "allow-declarations"],
    "prettier": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
